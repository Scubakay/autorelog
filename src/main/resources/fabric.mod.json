{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Reconnect automatically to a restarting Minecraft server when AFK!",
  "authors": [
    "Scubakay"
  ],
  "contributors": [
    "JustAlittleWolf",
    "NotRyken",
    "x3rt"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/autorelog",
    "sources": "https://github.com/Scubakay/autorelog",
    "issues": "https://github.com/Scubakay/autorelog/issues"
  },
  "license": "MIT",
  "icon": "assets/autorelog/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.scubakay.autorelog.AutoRelogClient"
    ]
  },
  "mixins": [
    "autorelog.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "minecraft": "${mcdep}",
    "${fabrickey}": "*"
  }
}